<div class="container marketing">

  <% if !@doctor_groups.empty? %>
    <% @doctor_groups.each do |row| %>
      <div class="courses">
        <% row.each do |doctor| %>
          <div class="course">
            <%= render doctor %>
          </div>
        <% end %>
      </div>
    <% end %>
  <% else %>
    <p>
    No Doctors found
    </p>
  <% end %>
</div>
